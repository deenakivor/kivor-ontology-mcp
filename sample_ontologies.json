{
  "infrastructure_ontology": {
    "name": "infrastructure_ontology",
    "version": "1.0.0",
    "category": "infrastructure",
    "description": "Ontology for infrastructure-related tickets including servers, networks, storage, and cloud resources",
    "tags": ["infrastructure", "server", "network", "storage", "cloud", "datacenter"],
    "priority": 80,
    "ontology_json": {
      "entities": [
        {
          "name": "Server",
          "type": "Infrastructure",
          "attributes": ["hostname", "ip_address", "os", "status"]
        },
        {
          "name": "Network",
          "type": "Infrastructure",
          "attributes": ["network_id", "subnet", "vlan"]
        },
        {
          "name": "Storage",
          "type": "Infrastructure",
          "attributes": ["storage_id", "capacity", "type"]
        },
        {
          "name": "LoadBalancer",
          "type": "Infrastructure",
          "attributes": ["lb_id", "algorithm", "health_check"]
        }
      ],
      "relationships": [
        {
          "source": "Server",
          "target": "Network",
          "type": "CONNECTS_TO"
        },
        {
          "source": "Server",
          "target": "Storage",
          "type": "USES"
        },
        {
          "source": "LoadBalancer",
          "target": "Server",
          "type": "ROUTES_TO"
        }
      ]
    }
  },
  "application_ontology": {
    "name": "application_ontology",
    "version": "1.0.0",
    "category": "application",
    "description": "Ontology for application-level tickets including code, deployment, and services",
    "tags": ["application", "software", "deployment", "service", "api"],
    "priority": 70,
    "ontology_json": {
      "entities": [
        {
          "name": "Application",
          "type": "Software",
          "attributes": ["app_name", "version", "environment"]
        },
        {
          "name": "Service",
          "type": "Software",
          "attributes": ["service_name", "port", "protocol"]
        },
        {
          "name": "API",
          "type": "Software",
          "attributes": ["endpoint", "method", "auth_type"]
        },
        {
          "name": "Deployment",
          "type": "Process",
          "attributes": ["deployment_id", "status", "timestamp"]
        }
      ],
      "relationships": [
        {
          "source": "Application",
          "target": "Service",
          "type": "DEPENDS_ON"
        },
        {
          "source": "Service",
          "target": "API",
          "type": "EXPOSES"
        },
        {
          "source": "Deployment",
          "target": "Application",
          "type": "DEPLOYS"
        }
      ]
    }
  },
  "database_ontology": {
    "name": "database_ontology",
    "version": "1.0.0",
    "category": "database",
    "description": "Ontology for database-related tickets including queries, schemas, and performance",
    "tags": ["database", "sql", "query", "schema", "performance", "backup"],
    "priority": 75,
    "ontology_json": {
      "entities": [
        {
          "name": "Database",
          "type": "DataStore",
          "attributes": ["db_name", "engine", "version"]
        },
        {
          "name": "Schema",
          "type": "DataStructure",
          "attributes": ["schema_name", "owner"]
        },
        {
          "name": "Table",
          "type": "DataStructure",
          "attributes": ["table_name", "row_count", "indexes"]
        },
        {
          "name": "Query",
          "type": "Operation",
          "attributes": ["query_id", "execution_time", "query_text"]
        }
      ],
      "relationships": [
        {
          "source": "Database",
          "target": "Schema",
          "type": "CONTAINS"
        },
        {
          "source": "Schema",
          "target": "Table",
          "type": "CONTAINS"
        },
        {
          "source": "Query",
          "target": "Table",
          "type": "ACCESSES"
        }
      ]
    }
  }
}
